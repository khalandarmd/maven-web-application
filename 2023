pipeline{
  agent any
  tools{
    maven 'maven3.9.0'
  }
  stages{
    stage('Checkout'){
      steps{
        git credentialsId: 'GitHub-Cred-2023', url: 'https://github.com/khalandarmd/maven-web-application.git'
      }
    }
    stage('Build'){
      steps{
        sh 'mvn clean install'
      }
    }
    stage('Deploy'){
      steps{
        deploy contextPath: 'http://13.232.122.220:8080/', war: '**/maven-web-application.war'
      }
    }
  }
}
